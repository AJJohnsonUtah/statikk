<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://www.ehcache.org/ehcache.xsd"
         updateCheck="true"
         monitoring="autodetect"
         dynamicConfig="true">

    <cache name="static-champions"
           maxElementsInMemory="50"
           eternal="false"
           overflowToDisk="false"
           timeToLiveSeconds="1800"
           timeToIdleSeconds="0"
           memoryStoreEvictionPolicy="LFU"
           transactionalMode="off">
    </cache>
    
    <cache name="realms"
           maxElementsInMemory="1"
           eternal="false"
           overflowToDisk="false"
           timeToLiveSeconds="40000"
           timeToIdleSeconds="0"
           memoryStoreEvictionPolicy="LFU"
           transactionalMode="off">
    </cache>
            
    <cache name="current-version"
           maxElementsInMemory="2"
           eternal="false"
           overflowToDisk="false"
           timeToLiveSeconds="10000"
           timeToIdleSeconds="0"
           memoryStoreEvictionPolicy="LFU"
           transactionalMode="off">
    </cache>

    <cache name="supported-versions"
           maxElementsInMemory="2"
           eternal="false"
           overflowToDisk="false"
           timeToLiveSeconds="10000"
           timeToIdleSeconds="0"
           memoryStoreEvictionPolicy="LFU"
           transactionalMode="off">
    </cache>

    <cache name="summoner-mastery"
           maxElementsInMemory="50"
           eternal="false"
           overflowToDisk="false"
           timeToLiveSeconds="1800"
           timeToIdleSeconds="0"
           memoryStoreEvictionPolicy="LFU"
           transactionalMode="off">
    </cache>

    <cache name="summoner-matches"
           maxElementsInMemory="50"
           eternal="false"
           overflowToDisk="false"
           timeToLiveSeconds="1800"
           timeToIdleSeconds="0"
           memoryStoreEvictionPolicy="LFU"
           transactionalMode="off">
    </cache>

    <cache name="champion-win-rates"
           maxElementsInMemory="50"
           eternal="false"
           overflowToDisk="false"
           timeToLiveSeconds="1800"
           timeToIdleSeconds="0"
           memoryStoreEvictionPolicy="LFU"
           transactionalMode="off">
    </cache>

    <cache name="summoner-name"
           maxElementsInMemory="50"
           eternal="false"
           overflowToDisk="false"
           timeToLiveSeconds="1800"
           timeToIdleSeconds="0"
           memoryStoreEvictionPolicy="LFU"
           transactionalMode="off">
    </cache>

    <cache name="current-game"
           maxElementsInMemory="50"
           eternal="false"
           overflowToDisk="false"
           timeToLiveSeconds="1800"
           timeToIdleSeconds="0"
           memoryStoreEvictionPolicy="LFU"
           transactionalMode="off">
    </cache>


</ehcache>